<h1>List Catalog</h1>

<table class="table1">
  <tr>
    <th>Name</th>
   
  </tr>

<% @catalogs.each do |catalog| %>
  <tr>
    <td><%= catalog.name %></td>
    <td><%= catalog.detail%></td>
    <td><%= link_to 'Edit', edit_catalog_path(catalog) %></td>
  </tr>
<% end %>
</table>

<br />
<p>
  Download:
  <%= link_to "CSV", catalogs_path(format: "csv") %> |
  <%= link_to "Excel", catalogs_path(format: "xls") %>
</p>